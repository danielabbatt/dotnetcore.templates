{
	"Logging": {
		"Serilog": {
			"Using": [ "Serilog.Sinks.Console" ],
			"MinimumLevel": "Debug",
			"WriteTo": [
				{
					"Name": "Console",
					"Args": {
						"outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] ({SourceContext}){NewLine}                {Message:lj}{NewLine}{Exception}"
					}
				},
				{
					"Name": "File",
					"Args": {
						"path": "%TEMP%\\Logs\\ExampleService.txt"
					}
				}
			],
			"Enrich": [
				"FromLogContext"
			]
		}
	}
}